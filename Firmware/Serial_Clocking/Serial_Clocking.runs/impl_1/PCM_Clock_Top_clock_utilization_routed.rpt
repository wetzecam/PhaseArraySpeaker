# Location of BUFG Primitives 
set_property LOC BUFGCTRL_X0Y17 [get_cells clk_source/inst/clkf_buf]
set_property LOC BUFGCTRL_X0Y16 [get_cells clk_source/inst/clkout1_buf]
set_property LOC BUFGCTRL_X0Y1 [get_cells DUT/LRCK_BUFF]
set_property LOC BUFGCTRL_X0Y0 [get_cells DUT/BCK_BUFF]

# Location of IO Primitives which is load of clock spine
set_property LOC IOB_X0Y40 [get_cells MCK_OBUF_inst]
set_property LOC IOB_X0Y39 [get_cells LRCK_OBUF_inst]
set_property LOC IOB_X0Y38 [get_cells BCK_OBUF_inst]

# Location of clock ports
set_property LOC IOB_X0Y76 [get_ports SCK]

# Clock net "clk_source/inst/clk_out1" driven by instance "clk_source/inst/clkout1_buf" located at site "BUFGCTRL_X0Y16"
#startgroup
create_pblock {CLKAG_clk_source/inst/clk_out1}
add_cells_to_pblock [get_pblocks  {CLKAG_clk_source/inst/clk_out1}] [get_cells -filter { PRIMITIVE_GROUP != I/O && IS_PRIMITIVE==1 && PRIMITIVE_LEVEL !=INTERNAL && NAME!=MCK_OBUF_inst} -of_object [get_pins -filter {DIRECTION==IN} -of_objects [get_nets -hierarchical -filter {PARENT=="clk_source/inst/clk_out1"}]]]
resize_pblock [get_pblocks {CLKAG_clk_source/inst/clk_out1}] -add {CLOCKREGION_X1Y0:CLOCKREGION_X1Y0}
#endgroup
